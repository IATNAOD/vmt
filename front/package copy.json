{
  "name": "redux-app",
  "version": "0.1.0",
  "private": true,
  "babel": {
    "presets": [
      "module:metro-react-native-babel-preset"
    ],
    "env": {
      "production": {}
    },
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-proposal-private-methods",
        {
          "loose": true
        }
      ],
      "transform-class-properties",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "transform-inline-environment-variables",
        {
          "include": [
            "NODE_ENV",
            "API"
          ]
        }
      ],
      [
        "@babel/plugin-proposal-optional-catch-binding"
      ]
    ]
  },
  "dependencies": {
    "apexcharts": "^3.28.1",
    "axios": "0.19.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap-daterangepicker": "^3.1.0",
    "connected-react-router": "^6.8.0",
    "cssnano": "^4.1.10",
    "history": "^4.10.1",
    "immutable": "^4.0.0-rc.12",
    "jquery": "^3.6.0",
    "konva": "^7.1.0",
    "multiselect-react-dropdown": "^2.0.5",
    "popper.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "rc-progress": "^3.1.3",
    "react": "^16.14.0",
    "react-apexcharts": "^1.3.9",
    "react-avatar-editor": "^11.0.9",
    "react-bootstrap-daterangepicker": "^7.0.0",
    "react-carousel-slider": "^2.0.13",
    "react-client-captcha": "^1.0.3",
    "react-country-flag": "^2.3.1",
    "react-dom": "^16.14.0",
    "react-draggable": "^4.4.3",
    "react-dropzone": "^11.2.4",
    "react-helmet": "^6.1.0",
    "react-konva": "^16.13.0-6",
    "react-paginate": "^7.1.3",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-svg-worldmap": "^1.1.0",
    "react-toastify": "^6.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "seamless-immutable": "^7.1.4",
    "sweetalert-react": "^0.4.11",
    "universal-cookie": "^4.0.4",
    "use-image": "^1.0.6"
  },
  "scripts": {
    "start": "parcel index.html",
    "build:release": "parcel build index.html"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-optional-catch-binding": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "autoprefixer": "^10.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "metro-react-native-babel-preset": "^0.56.4",
    "postcss": "^8.2.1",
    "postsass": "^0.1.2",
    "sass": "^1.32.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
